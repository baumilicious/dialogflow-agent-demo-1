{
  "name": "39897a42-43ff-454a-9b14-7cdc3a24012f",
  "displayName": "book_an_appointment",
  "form": {
    "parameters": [ {
      "displayName": "Location",
      "required": true,
      "entityType": "@sys.location",
      "fillBehavior": {
        "initialPromptFulfillment": {
          "messages": [ {
            "text": {
              "text": [ "What store are you interested in going to?" ]
            },
            "languageCode": "en"
          } ]
        }
      }
    }, {
      "displayName": "Date",
      "required": true,
      "entityType": "@sys.date",
      "fillBehavior": {
        "initialPromptFulfillment": {
          "messages": [ {
            "text": {
              "text": [ "What day of the week would you like the appointment for?\n" ]
            },
            "languageCode": "en"
          } ]
        }
      }
    }, {
      "displayName": "Time",
      "required": true,
      "entityType": "@sys.time",
      "fillBehavior": {
        "initialPromptFulfillment": {
          "messages": [ {
            "text": {
              "text": [ "What time is convenient for you? Our available time slots are mornings, from 9:00 AM to 11:00 AM\n" ]
            },
            "languageCode": "en"
          } ]
        }
      }
    }, {
      "displayName": "Name",
      "required": true,
      "entityType": "@sys.person",
      "fillBehavior": {
        "initialPromptFulfillment": {
          "messages": [ {
            "text": {
              "text": [ "What name should I put in the reservation?\n" ]
            },
            "languageCode": "en"
          } ]
        }
      }
    } ]
  },
  "transitionRoutes": [ {
    "condition": "$page.params.status = \"FINAL\"",
    "triggerFulfillment": {
      "messages": [ {
        "text": {
          "text": [ "Great $session.params.name. You are all set for next $session.params.date at $session.params.time in our $session.params.location\n" ]
        },
        "languageCode": "en"
      } ]
    },
    "targetPage": "Confirm Appointment",
    "name": "d4a06d28-6bd8-4e22-8763-55a27dd7d614"
  } ]
}
